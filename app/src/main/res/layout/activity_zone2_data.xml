                android:paddingTop="50dp"
                android:textSize="20dp"/>

            <com.github.chrisbanes.photoview.PhotoView
                android:id="@+id/Zone2infrustructure_img"
                android:layout_width="wrap_content"
                android:layout_height="300dp"
                app:srcCompat="@drawable/zone_1_infrustructure"
                android:layout_below="@+id/Zone_2_Title_11"/>


        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
