                android:layout_height="wrap_content"
                android:layout_below="@+id/Zone2riligious_img"
                android:layout_weight="1"
                android:ems="10"
                android:enabled="false"
                android:inputType="textPersonName"
                android:text="Kilbati Rasu Water Schemes"
                android:textAlignment="center"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"

                android:paddingTop="50dp"
                android:textSize="20dp"/>

            <com.github.chrisbanes.photoview.PhotoView
                android:id="@+id/Zone2water_img"
                android:layout_width="wrap_content"
                android:layout_height="300dp"
                app:srcCompat="@drawable/zone_2_water"
                android:layout_below="@+id/Zone_2_Title_5"/>

            <TextView
                android:id="@+id/Zone_2_Title_6"
                style="@style/AlertDialog.AppCompat.Light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Zone2water_img"
                android:layout_weight="1"
                android:ems="10"
                android:enabled="false"
                android:inputType="textPersonName"
                android:text="Kilbati Rasu Rural Villages"
                android:textAlignment="center"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"

                android:paddingTop="50dp"
                android:textSize="20dp"/>

            <com.github.chrisbanes.photoview.PhotoView
                android:id="@+id/Zone2village_img"
                android:layout_width="wrap_content"
                android:layout_height="300dp"
                app:srcCompat="@drawable/zone_1_villages"
                android:layout_below="@+id/Zone_2_Title_6"/>


            <TextView
                android:id="@+id/Zone_2_Title_7"
                style="@style/AlertDialog.AppCompat.Light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Zone2village_img"
                android:layout_weight="1"
                android:ems="10"
                android:enabled="false"
                android:inputType="textPersonName"
                android:text="Urbans of Kilbati"
                android:textAlignment="center"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"

                android:paddingTop="50dp"
                android:textSize="20dp"/>

            <com.github.chrisbanes.photoview.PhotoView
                android:id="@+id/Zone2urban_img"
                android:layout_width="wrap_content"
