                android:textAlignment="center"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"

                android:paddingTop="50dp"
                android:textSize="20dp"/>

            <com.github.chrisbanes.photoview.PhotoView
                android:id="@+id/Zone2pub_img"
                android:layout_width="wrap_content"
                android:layout_height="300dp"
                app:srcCompat="@drawable/zone_1_public"
                android:layout_below="@+id/Zone_2_Title_9"/>


            <!--Commercial Services of Kilbati-->

            <TextView
                android:id="@+id/Zone_2_Title_10"
                style="@style/AlertDialog.AppCompat.Light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Zone2pub_img"
                android:layout_weight="1"
                android:ems="10"
                android:enabled="false"
                android:inputType="textPersonName"
                android:text="Commercial Services of Kilbati"
                android:textAlignment="center"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"

