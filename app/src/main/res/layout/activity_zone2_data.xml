                android:textSize="20dp"/>

            <com.github.chrisbanes.photoview.PhotoView
                android:id="@+id/Zone2comm_img"
                android:layout_width="wrap_content"
                android:layout_height="300dp"
                app:srcCompat="@drawable/zone_1_commercial"
