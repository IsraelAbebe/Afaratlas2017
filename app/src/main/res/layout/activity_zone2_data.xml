<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.gmail.se.israel.abebe.Zone2_data">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/Zone_2_Title_main"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:enabled="false"
                android:textSize="30dp"
                android:inputType="textPersonName"
                android:text="Kilbati Rasu "
                android:textAlignment="center"
                android:textStyle="bold"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"/>

            <TextView
                android:id="@+id/Zone_2_Title_1"
                style="@style/AlertDialog.AppCompat.Light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Zone_2_Title_main"
                android:layout_weight="1"
                android:ems="10"
                android:textSize="20dp"
                android:enabled="false"
                android:inputType="textPersonName"
                android:text="Kilbati Rasu Educational Facilities"
                android:textAlignment="center"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                />

            <TextView
                android:id="@+id/Zone_2_disc_1"
                style="@style/AlertDialog.AppCompat.Light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Zone_2_Title_1"
                android:layout_weight="1"
                android:ems="10"
                android:textSize="15dp"
                android:enabled="false"
                android:text="@string/zone2_disc"
                android:maxLines="9"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                />

            <com.github.chrisbanes.photoview.PhotoView
                android:id="@+id/Zone2educational_img"
                android:layout_width="wrap_content"
                android:layout_height="300dp"
                app:srcCompat="@drawable/zone_2_education"
                android:layout_below="@+id/Zone_2_disc_1"/>

            <TextView
                android:id="@+id/Zone_2_Title_2"
                style="@style/AlertDialog.AppCompat.Light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Zone2educational_img"
                android:layout_weight="1"
                android:ems="10"
                android:enabled="false"
                android:inputType="textPersonName"
                android:text="Kilbati Rasu Animal Health Facilities"
                android:textAlignment="center"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"

                android:paddingTop="50dp"
                android:textSize="20dp"/>

            <com.github.chrisbanes.photoview.PhotoView
                android:id="@+id/Zone2helth_img"
                android:layout_width="wrap_content"
                android:layout_height="300dp"
                app:srcCompat="@drawable/zone_2_animal"
                android:layout_below="@+id/Zone_2_Title_2"/>

            <TextView
                android:id="@+id/Zone_2_Title_3"
                style="@style/AlertDialog.AppCompat.Light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Zone2helth_img"
                android:layout_weight="1"
                android:ems="10"
                android:enabled="false"
                android:inputType="textPersonName"
                android:text="Kilbati Rasu Health Facilities"
                android:textAlignment="center"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"

                android:paddingTop="50dp"
                android:textSize="20dp"/>

            <com.github.chrisbanes.photoview.PhotoView
                android:id="@+id/Zone2helthfacilities_img"
                android:layout_width="wrap_content"
                android:layout_height="300dp"
                app:srcCompat="@drawable/zone_2_helth"
                android:layout_below="@+id/Zone_2_Title_3"/>



            <TextView
                android:id="@+id/Zone_2_Title_4"
                style="@style/AlertDialog.AppCompat.Light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Zone2helthfacilities_img"
                android:layout_weight="1"
                android:ems="10"
                android:enabled="false"
                android:inputType="textPersonName"
                android:text="Kilbati Rasu Religious Institutions"
                android:textAlignment="center"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"

                android:paddingTop="50dp"
                android:textSize="20dp"/>

            <com.github.chrisbanes.photoview.PhotoView
                android:id="@+id/Zone2riligious_img"
                android:layout_width="wrap_content"
                android:layout_height="300dp"
                app:srcCompat="@drawable/zone_2_religion"
                android:layout_below="@+id/Zone_2_Title_4"/>

            <TextView
                android:id="@+id/Zone_2_Title_5"
                style="@style/AlertDialog.AppCompat.Light"
