<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.gmail.se.israel.abebe.Zone1_data">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/Zone_1_Title_main"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:enabled="false"
                android:textSize="30dp"
                android:inputType="textPersonName"
                android:text="Awsi Rasu "
                android:textAlignment="center"
                android:textStyle="bold"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"/>

            <TextView
                android:id="@+id/Zone_1_Title_1"
                style="@style/AlertDialog.AppCompat.Light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Zone_1_Title_main"
                android:layout_weight="1"
                android:ems="10"
                android:textSize="20dp"
                android:enabled="false"
                android:inputType="textPersonName"
                android:text="Awsi Rasu Educational Facilities"
                android:textAlignment="center"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                />

            <TextView
                android:id="@+id/Zone_1_disc_1"
                style="@style/AlertDialog.AppCompat.Light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Zone_1_Title_1"
                android:layout_weight="1"
                android:ems="10"
                android:textSize="15dp"
                android:enabled="false"
                android:text="@string/zone1_disc"
                android:maxLines="9"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                />

            <com.github.chrisbanes.photoview.PhotoView
                android:id="@+id/zone1educational_img"
                android:layout_width="wrap_content"
                android:layout_height="300dp"
                app:srcCompat="@drawable/zone_1_education"
                android:layout_below="@+id/Zone_1_disc_1"/>

            <TextView
                android:id="@+id/Zone_1_Title_2"
                style="@style/AlertDialog.AppCompat.Light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/zone1educational_img"
                android:layout_weight="1"
                android:ems="10"
                android:enabled="false"
                android:inputType="textPersonName"
                android:text="Awsi Rasu Animal Health Facilities"
                android:textAlignment="center"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                android:paddingTop="50dp"
                android:textSize="20dp"/>

            <com.github.chrisbanes.photoview.PhotoView
                android:id="@+id/zone1helth_img"
                android:layout_width="wrap_content"
                android:layout_height="300dp"
                app:srcCompat="@drawable/zone_1_animal"
                android:layout_below="@+id/Zone_1_Title_2"/>

            <TextView
                android:id="@+id/Zone_1_Title_3"
                style="@style/AlertDialog.AppCompat.Light"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/zone1helth_img"
                android:layout_weight="1"
                android:ems="10"
                android:enabled="false"
                android:inputType="textPersonName"
                android:text="Awsi Rasu Health Facilities"
                android:textAlignment="center"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                android:paddingTop="50dp"
                android:textSize="20dp"/>

